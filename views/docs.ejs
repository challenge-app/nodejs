									<tr>
										<td><code class="normal">$generalVotes</code></td>
										<td><span class="type">Integer</span></td>
										<td>All thumbs up.</td>
									</tr>
									<tr>
										<td><code class="normal">$challenges</code></td>
										<td>
											<span class="type">Array</span> or 
											<a href="#" data-trigger="challenge-model">Challenges</a>
										</td>
										<td>Challenges with this ChallengeInfo.</td>
									</tr>
								</tbody>
							</table>

					<hr/>
					<h1>Models</h1>
					<section id="user-model">
						<article class="infos">
							<h2>User model</h2>
							<p>Here you will see the schema for User model.</p>
							<table class="table table-bordered table-stripped">
								<thead>
									<td>Parameter</td>
									<td>Type</td>
									<td>Description</td>
								</thead>
								<tbody>
									<tr>
										<td><code class="normal">$_id</code></td>
										<td><span class="type">String</span></td>
										<td>User id autogenerated.</td>
									</tr>
									<tr>
										<td><code class="normal">$authenticationToken</code></td>
										<td><span class="type">String</span></td>
										<td>User current (valid) token.</td>
									</tr>
									<tr>
										<td><code class="normal">$email</code></td>
										<td><span class="type">String</span></td>
										<td>User email (primary key).</td>
									</tr>
									<tr>
										<td><code class="normal">$password</code></td>
										<td><span class="type">String</span></td>
										<td>User password (hashed and salted).</td>
									</tr>
									<tr>
										<td><code class="normal">$friends</code></td>
										<td>
											<span class="type">(Array) String</span> or 
											<a href="#" data-trigger="user-model">(Array) Users</a>
										</td>
										<td>User friends ($ids or Users).</td>
									</tr>
								</tbody>
							</table>
							<h5>Example</h5>
							<div class="blockcode json">
								<p>{</p>
								<p>_id : "52cc9cd3515sab4d1554de49",</p>
								<p>authenticationToken : "$2a$12$tL1ViLRmodnC1d4oAbFzIOYdd2BO5eutgdhI39OsqGVBnRWaF2E2O",</p>
								<p>email : "teste@teste.com",</p>
								<p>password : "protected",</p>
								<p>friends : [
								<p class="tab1">"52ce527cfebb200c5758f58d",</p>
								<p class="tab1">"52ce527ffebb200c5758f58f"</p>
								<p>]</p>
								<p>}</p>
							</div>
						</article>
					</section>
					<hr/>
					<section id="challenge-model">
						<article class="infos">
							<h2>Challenge model</h2>
							<p>Here you will see the schema for Challenge model.</p>
							<table class="table table-bordered table-stripped">
								<thead>
									<td>Parameter</td>
									<td>Type</td>
									<td>Description</td>
								</thead>
								<tbody>
									<tr>
										<td><code class="normal">$_id</code></td>
										<td><span class="type">String</span></td>
										<td>Challenge id autogenerated.</td>
									</tr>
									<tr>
										<td><code class="normal">$info</code></td>
										<td>
											<a href="#" data-trigger="challengeInfo-model">ChallengeInfo</a>
										</td>
										<td>Challenge info.</td>
									</tr>
									<tr>
										<td><code class="normal">$sender</code></td>
										<td>
											<span class="type">String</span> or 
											<a href="#" data-trigger="user-model">User</a>
										</td>
										<td>The challenger.</td>
									</tr>
									<tr>
										<td><code class="normal">$receiver</code></td>
										<td>
											<span class="type">String</span> or 
											<a href="#" data-trigger="user-model">User</a>
										</td>
										<td>Who has been challenged.</td>
									</tr>
									<tr>
										<td><code class="normal">$reward</code></td>
										<td><span class="type">Integer</span></td>
										<td>Reputation as reward.</td>
									</tr>
									<tr>
										<td><code class="normal">$votes</code></td>
										<td><span class="type">Integer</span></td>
										<td>Thumbs up.</td>
									</tr>
								</tbody>
							</table>
							<h5>Example</h5>
							<div class="blockcode json">
								<p>{</p>
								<p>_id: "52ce58ff3febf3205aa0d50e",</p>
								<p>info: {</p>
								<p class="tab1">_id: 52ce527cfebb200c5758f58d,</p>
								<p class="tab1">description: "I dear you to...",</p>
								<p class="tab1">votes: 125</p>
								<p>},</p>
								<p>sender: "52ce527afebb200c5758f58b",</p>
								<p>receiver: {</p>
								<p class="tab1">_id: 52ce527cfebb200c5758f58d,</p>
								<p class="tab1">email: "friend@teste.com"</p>
								<p>},</p>
								<p>reward: 25,</p>
								<p>}</p>
							</div>
						</article>
					</section>
					<hr/>
					<section id="challengeInfo-model">
						<article class="infos">
							<h2>ChallengeInfo model</h2>
							<p>Here you will see the schema for ChallengeInfo model.</p>
							<table class="table table-bordered table-stripped">
								<thead>
									<td>Parameter</td>
									<td>Type</td>
									<td>Description</td>
								</thead>
								<tbody>
									<tr>
										<td><code class="normal">$_id</code></td>
										<td><span class="type">String</span></td>
										<td>ChallengeInfo id autogenerated.</td>
									</tr>
									<tr>
										<td><code class="normal">$description</code></td>
										<td><span class="type">String</span></td>
										<td>Description of the challenge.</td>

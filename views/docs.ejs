							<div class="blockcode json">
								<p>{</p>
								<p>_id: 52ce527cfebb200c5758f58d,</p>
								<p>description: "I dear you to...",</p>
								<p>votes: 0,</p>
								<p>challenges: []</p>
								<p>}</p>
							</div>
						</article>
					</section>
				</div>
			</div>
